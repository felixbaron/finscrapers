        "ts-node": "^8.0.1",
        "typescript": "^3.2.4"
    },
    "dependencies": {
        "commander": "^2.19.0",
        "elasticsearch": "^15.3.1",
        "puppeteer": "^1.11.0"
    },
    "bugs": {
        "url": "https://github.com/finscrapers/finscrapers/issues"
    },
